<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="UserNameStyle" parent="TextAppearance.MaterialComponents.Caption">
        <item name="android:textColor">@color/pankhuri_black</item>

    </style>

    <style name="UserDetailTextStyleBlack" parent="TextAppearance.MaterialComponents.Caption">
        <item name="android:textColor">@color/pankhuri_black</item>
        <item name="android:textSize">12sp</item>
        <item name="android:drawablePadding">4dp</item>
        <item name="android:gravity">center_vertical</item>

    </style>

    <style name="tabText" parent="TextAppearance.Design.Tab">
        <item name="textAllCaps">false</item>
        <item name="tabTextColor">@color/black</item>
        <item name="indicatorColor">@color/pink</item>
    </style>

    <style name="roundedImageViewRounded">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
</resources>